{% extends "::layout/layout.html.twig" %}
{% block body %}
    {% for objectif in objectifs %}
        <div class="container white col-lg-12 padder-t2 DivTitre">
            <h2 class="LigneTitre Titre"><span class="LigneSpan">{{ objectif.nom }}</span></h2>
        </div>
        <section id="Objectif" class="col-xs-12 col-sm-12 col-md-12 col-lg-9 ">
            <div class="col-xs-5 col-sm-5 col-md-5 col-lg-offset-1 col-lg-5 Description">
                <p>{{ objectif.description }}</p>
            </div>
            <img id="ImgLieu" class="col-xs-4 col-sm-4 col-md-4 col-lg-4" src="{{ asset(objectif.media.path) }}">
        </section>
    {% endfor %}
{% endblock %}