{% extends "::layout/layout.html.twig" %}
{% block body %}
    {% for objectif in objectifs %}
        <div id="DivTitreContact" class="container white col-lg-12 padder-t2">
            <h2 id="TitreContact" class="LigneTitre"><span class="LigneSpan">{{ objectif.nom }}</span></h2>
        </div>
        <section id="Objectif" class="col-xs-12 col-sm-12 col-md-12 col-lg-9 ">
            {#<h3 id="TitreObjectif" class="col-xs-4 col-sm-4 col-md-4 col-lg-4"> {{ objectif.nom }} ? </h3>#}
            <div id="DescriptionObjectif" class="col-xs-5 col-sm-5 col-md-5 col-lg-offset-1 col-lg-5">
                <p>{{ objectif.description }}</p>
            </div>
        </section>
    {% endfor %}
{% endblock %}